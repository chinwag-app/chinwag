{{#paper-toolbar}}
	<div class="md-toolbar-tools">
		{{#link-to "index" class="toolbar-back"}}{{paper-icon icon="arrow-back"}}{{/link-to}}
		<span>Start conversation</span>
	</div>
{{/paper-toolbar}}
{{#paper-list classNames="conversation-list"}}
	{{#each}}
		{{#paper-item classNames="conversation-item"}}
			{{#paper-tile-left}}
				{{#if avatar}}
					<img {{bind-attr src=avatar alt=prettyName title=prettyName}} width="30" height="30" class="face">
				{{else}}
					{{avatar-tile name=prettyName class="face" alt=prettyName title=prettyName}}
				{{/if}}
			{{/paper-tile-left}}

			{{#paper-tile-content}}
				<h3>{{prettyName}}</h3>
				<p>{{jid}}</p>
			{{/paper-tile-content}}
		{{/paper-item}}
		{{paper-divider inset=true}}
	{{/each}}
{{/paper-list}}
