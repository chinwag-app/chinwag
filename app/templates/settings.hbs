{{#paper-toolbar}}
	<div class="md-toolbar-tools">
		{{#link-to "index" class="toolbar-back"}}{{paper-icon icon="arrow-back"}}{{/link-to}}
		<span>Settings</span>
	</div>
{{/paper-toolbar}}
{{#paper-list classNames="conversations"}}
	{{#paper-item classNames="conversation"}}
		{{#paper-tile-content}}
			<h3>Version</h3>
			<p>Chinwag {{version}}</p>
		{{/paper-tile-content}}
	{{/paper-item}}
{{/paper-list}}